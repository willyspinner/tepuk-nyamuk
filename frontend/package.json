  "scripts": {
    "build:dev": "source ../shared/.development.localhost.env ;webpack --env.mode development --env.APPCS_HOST \"$APPCS_HOST\" --env.APPCS_PORT $APPCS_PORT --env.GMS_HOST \"$GMS_HOST\" --env.GMS_PORT $GMS_PORT ",
    "build:prod-local": "source ../shared/.production.localhost.env; webpack -p  --optimize-minimize --optimize-dedupe --env.mode production --env.APPCS_HOST \"$APPCS_HOST\" --env.APPCS_PORT $APPCS_PORT --env.GMS_HOST \"$GMS_HOST\" --env.GMS_PORT $GMS_PORT",
    "build:prod-host": " source ../shared/.production.host.env; webpack -p  --optimize-minimize --optimize-dedupe --env.mode production --env.APPCS_HOST \"$APPCS_HOST\" --env.APPCS_PORT $APPCS_PORT --env.GMS_HOST \"$GMS_HOST\" --env.GMS_PORT $GMS_PORT",
    "build:prod-profile": "source ../shared/.production.localhost.env; webpack -p  --optimize-minimize --optimize-dedupe --env.mode production_profile",
    "serve-httpster": " httpster -d public/",
    "build:prod-local_and_serve": "npm run build:prod-local && npm run serve-httpster",
    "build:prod-host_and_serve": "npm run build:prod-host && npm run serve-httpster",
    "dev-server": " source ../shared/.development.localhost.env; webpack-dev-server --mode development --env.APPCS_HOST \"$APPCS_HOST\" --env.APPCS_PORT $APPCS_PORT --env.GMS_HOST \"$GMS_HOST\" --env.GMS_PORT $GMS_PORT ",
    "dev-server-LAN": "source ../shared/.development.lan.env; command -v notif >/dev/null 2>&1 &&  notif 'webpack' \"Devserver on $ipaddr\" 'daniel' & webpack-dev-server  --mode development --host \"$ipaddr\" --env.APPCS_HOST \"$APPCS_HOST\" --env.APPCS_PORT $APPCS_PORT --env.GMS_HOST \"$GMS_HOST\" --env.GMS_PORT $GMS_PORT ",
    "jest": "cross-env NODE_ENV=test jest --config=jest.config.json",
    "jest-clean": "jest --config=jest.config.json -u",
    "start": "node server/server.js",
    "heroku-postbuild": "yarn run build:prod",
    "build:dev-and-serve": "webpack --env development && node server/server.js"
  },
  "name": "tepuknyamuk_web_frontend",
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>/src/tests/setupTests.js"
